# text_orig = ― Aramè, ianê requè ikè, iámanù riri ianê requè cerá?
# text_source = p. 33
# text_por = ― Então aqui temos uma vida, e depois da nossa morte teremos outra?
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:5:6:56
# text = ― Yawé tẽ. Yané rikwé iwí-pe; yané rikwé yamanú rirí.
# text_orig = ― Iauè-ten. Ianê requé ïuhïpe; ianê requé ia-manú riri.
# text_source = p. 33
# text_por = ― É verdade. Temos uma vida na terra, e outra depois da morte.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:5:7:57
# text = ― Yamanú ramé, maã kití yasú?
# text_orig = ― Ià manú ramé, mahá-kïtï iáçù?
# text_source = p. 33
# text_por = ― Quando morrermos, para onde vamos?
# text_sec = ― Yamanú ramé, makití yasú?
# text_por_sec = ― Quando morremos, para onde vamos?
# text_sec_source = Avila (2021)
# text_por_sec_source = Avila (2021)
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:5:8:58
# text = ― Mira katú iwaka kití, mira puxi yurupariratá kití.
# text_orig = ― Mira catú iuaca kïtï, mira puxi iurupariratá kïtï.
# text_source = p. 33
# text_por = ― Os bons vão para o Céu, e os maus para o inferno.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:6:1:59
# text = ― Yepé mira uyusalvari kwáu, tiramé uruyari, tiramé uyumuseruka?
# title = UWATARI RETÉ SIKÚ-KATÚ SUÍ, AWÁ YU-SALVARI ARAMA.
# title_orig = UATARl RETÉ CECUCATÙ ÇUI, AUÁ IU-SALVARI ARAMA.
# title_por = NECESSIDADE DAS BOAS OBRAS, PARA ALGUÉM SALVAR-SE.
# text_orig = ― Iepé mira iu-salvari quau, 'tiramé u-ruiari, 'tiramé iu muceruca?
# text_source = p. 35
# text_por = ― Uma pessoa pode salvar-se sem crer, nem baptizar-se?
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:6:2:60
# text = ― Tiana! Uwatari reté uruyari rẽ, useruka rẽ.
# text_orig = ― 'Tiana! Uatarireté u-ruiari rain, u-ceruca rain.
# text_source = p. 35
# text_por = ― Absolutamente não! É necessário crer e baptizar-se.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:6:3:61
# text = ― Maí taá yakwáu [ia-uhikí] iwaka upé?
# text_orig = ― Mahi-tahá iá quau ia-uhikí iuáca upé?
# text_source = p. 35
# text_por = ― Como podemos alcançar o Céu?
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:6:4:62
# text = Jesu Christu ruí rikuyara, yamunhã Tupana umunú waá, yané suí.
# text_orig = ― Jesu-Christu ruhï recuiára, iá-munhãn Tupana u-munu uahá, ianê çui.
# text_source = p. 35
# text_por = ― Mediante o preço do sangue de Jesus Cristo, e fazendo da nossa parte o que Deus manda.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:7:1:63
# title = KURUSÁ RANGAWA.
# title_orig = CURUÇÁ-RANGÀUA.
# title_por = SINAL DA CRUZ.
# text = ― Santa Kurusá rangawa rupí, yané pisirú Tupana, yané Yara, yané ruanhana.
# text_orig = ― Santa Curuçá rangáua rupi, ianè pïcïrú Tupana, ianê Iara, ianê ruîanhana.
# text_source = p. 35
# text_por = ― Pelo sinal da Santa Cruz, livre-nos Deus, Nosso Senhor, dos nossos inimigos.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:7:2:64
# text = ― Paya rera upé, Taíra yuíri Espírito Santo yuíri.
# text_orig = ― Paia rêra upé, Tahïra ïuïri Espíritu Santu ïuïri.
# text_source = p. 35
# text_por = ― Em nome do Pai, e do Filho, e do Espírito Santo.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:7:3:65
# text = ― Yawé.
# text_orig = ― Iaué.
# text_source = p. 35
# text_por = ― Amém.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:8:1:66
# text = Yané Paya, iné reikú waá iwaka upé, ne rera yumuité; yuri ne yarasawa yané árupi; yumunhã ne remutara iké iwí-pe, maí iwaka upé; yané miapé ara yawé remeẽ yané arama uií ara; reperduari yané watari, maí yané yaperduari amú-itá watari; teẽ rexari yané [iaari] sikú-puxi kití; repisirú yané upaĩ maã puxi suí.
# title = YANÉ PAYA
# title_orig = IANÊ PAIA
# title_por = PADRE-NOSSO 
# text_orig = Ianê Paia, inè rê-icú uahá iuáca upé, ne rêra iu-muité; iúri nê iáraçáua ianê árape; iu-munhan ne remutàra iké ïuhïpe, mahi iuáca upé; ianê miapé ara-iaué remehen ianê arama uhihi ara; re-perduari ianê uatari, mahi ianê ia perduari amùitá uatari; tehen re-xiari ianê iáari cecúpuxi kïtï; re-pïcïrú ianê upãin mahãpuxi çui.
# text_source = p. 37
# text_por = Pai nosso, que estás no Céu, santificado seja o teu nome; venha a nós o teu reino; seja feita a tua vontade, assim na terra como no Céu; o pão nosso de cada dia nos dá hoje; perdoa as nossas dívidas, assim como nós perdoamos a dívida dos outros; não nos deixes cair em tentação; mas livra-nos de todo o mal.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:8:2:67
# text = Yawé.
# text_orig = Iaué.
# text_source = p. 37
# text_por = Amém.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:9:1:68
# text = Ave Maria, i pura graça suí, yané Yara ne irumu; yumumeú katú iné upaĩ kunhã pitérupi, yumumú katí iwá ne marika suí, Jesus.
# title = AVE MARIA
# title_orig = AVE MARIA
# title_por = AVE MARIA
# text_orig = Ave-Maria, ipura graça çui, ianê Iàra ne iruma; iu-mumehun-catú iné upãin cunhan pïtérape, iu-mumehun-catú iuhá nê marica çui, Jesus.
# text_source = p. 37
# text_por = Ave-Maria, cheia de graça, o Senhor seja contigo; bendita és tu entre todas as mulheres, bendito é o fruto do teu ventre, Jesus.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:9:2:69
# text = Santa Maria Tupana Manha reyururé yané resé pecadori rupí, kuíri manú ramé yuíri.
# text_orig = Santa Maria Tupana Manha reiururé ianérecê pecadori rupi, cuhïri, ianê manú ramé ïuïri.
# text_source = p. 37
# text_por = Santa Maria, Mãe de Deus, roga por nós pecadores, agora e na hora da nossa morte.
# text_orig_transcriber = Davi Sampaio Santos
# text_por_modernizer = Davi Sampaio Santos
# text_modernizer = Letícia Farias Nunes

# sent_id = Aguiar1898:9:3:70
